#include<stdio.h>

#include<stdlib.h>

void main(int argc, char *argv[])

{

    int x,y,sum;

    if(argc==3)

    {

        x=atoi(argv[1]);

        y=atoi(argv[2]);

        sum=x+y;

        printf("Sum =%d",sum);

    }

    else

    {

        printf("Enter Two Numbersâ€¦");

    }

}

 

 

 

 

